# ===== Singpass (staging placeholders) =====
SINGPASS_ISSUER=https://stg-id.singpass.gov.sg
SINGPASS_AUTHORIZATION_ENDPOINT=https://stg-id.singpass.gov.sg/auth
SINGPASS_TOKEN_ENDPOINT=https://stg-id.singpass.gov.sg/token
SINGPASS_USERINFO_ENDPOINT=https://stg-id.singpass.gov.sg/userinfo
SINGPASS_CLIENT_ID=your-sp-client-id
# JWK (private key) as single-line JSON for private_key_jwt (include kid, alg=RS256)
SINGPASS_CLIENT_JWK={"kty":"RSA","kid":"abc","n":"...","e":"AQAB","d":"...","alg":"RS256"}
SINGPASS_REDIRECT_URI=https://your-domain.app.airbase.sg/api/auth/callback?provider=singpass


# ===== TechPass (optional; same flow) =====
TECHPASS_ISSUER=https://login.microsoftonline.com/your-tenant/v2.0
TECHPASS_AUTHORIZATION_ENDPOINT=https://login.microsoftonline.com/your-tenant/oauth2/v2.0/authorize
TECHPASS_TOKEN_ENDPOINT=https://login.microsoftonline.com/your-tenant/oauth2/v2.0/token
TECHPASS_USERINFO_ENDPOINT=https://graph.microsoft.com/oidc/userinfo
TECHPASS_CLIENT_ID=your-tp-client-id
TECHPASS_CLIENT_JWK={"kty":"RSA","kid":"abc","n":"...","e":"AQAB","d":"...","alg":"RS256"}
TECHPASS_REDIRECT_URI=https://your-domain.app.airbase.sg/api/auth/callback?provider=techpass


# ===== Session cookie (JWE) =====
SESSION_JWE_SECRET=super-long-random-32-bytes-or-more
SESSION_COOKIE_NAME=ab_sess


# ===== DynamoDB (use custom prefixes to avoid reserved names) =====
CUSTOM_AWS_REGION=ap-southeast-1
CUSTOM_AWS_ACCESS_KEY_ID=xxx
CUSTOM_AWS_SECRET_ACCESS_KEY=xxx
DDB_TABLE=portal-items


# ===== Azure OpenAI =====
